{
    "name": "culturekings/afterpay",
    "description": "Afterpay API integration",
    "type": "library",
    "authors": [
        {
            "name": "Jared Fraser",
            "email": "j.fraser@culturekings.com.au"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:culturekings/Symfony2-coding-standard.git"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "guzzlehttp/guzzle": "^6.2",
        "jms/serializer": "^1.2",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "henrikbjorn/phpspec-code-coverage": "^3.0",
        "symfony/var-dumper": "^3.1",
        "squizlabs/php_codesniffer": "^2.6",
        "culturekings/symfony2-coding-standard": "^2.10",
        "sebastian/phpcpd": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "CultureKings\\AfterPay\\": "src/"
        },
        "psr-0": {
            "": "src"
        }
    }
}
